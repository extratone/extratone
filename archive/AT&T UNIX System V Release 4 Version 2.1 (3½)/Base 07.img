	(ip)->i_count++; \
	(ip)->i_flag |= ILOCKED; \
	if (((ip)->i_vnode.v_flag & VISSWAP) != 0){ \
		curproc->p_swlocks++;	\
		curproc->p_flag |= SSWLOCKS;\
	} \
}

#define	IUNLOCK(ip) { \
	if (--(ip)->i_count < 0) \
		panic("IUNLOCK"); \
	if (((ip)->i_vnode.v_flag & VISSWAP) != 0) \
		if (--curproc->p_swlocks == 0)  \
			curproc->p_flag &= ~SSWLOCKS; \
	if ((ip)->i_count == 0) { \
		(ip)->i_flag &= ~ILOCKED; \
		if ((ip)->i_flag & IWANT) { \
			(ip)->i_flag &= ~IWANT; \
