#if defined(__STDC__)
extern int segu_create(struct seg *, void *);
extern addr_t segu_get(struct proc *, int);
extern void segu_release(struct proc *);
#else
extern int	segu_create();
extern addr_t	segu_get();
extern void	segu_release();
#endif /* __STDC__ */

#endif	/* _KERNEL */

#endif	/* _VM_SEG_U_H */
