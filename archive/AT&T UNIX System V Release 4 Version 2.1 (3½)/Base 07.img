#define XT_MSTAT_NOPROCESS      0       /* Do not process virtual intr  */
#define XT_MSTAT_PROCESS        1       /* Process virtual intr         */
#define XT_MSTAT_OPSAVED        2       /* v86 program opcode saved     */
