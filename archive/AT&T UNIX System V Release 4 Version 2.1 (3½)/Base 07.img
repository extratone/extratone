#define IMARK(ip) { \
	if (hrestime.tv_sec > iuniqtime.tv_sec || \
		hrestime.tv_nsec/1000 > iuniqtime.tv_usec) { \
		iuniqtime.tv_sec = hrestime.tv_sec; \
		iuniqtime.tv_usec = hrestime.tv_nsec/1000; \
	} else { \
		iuniqtime.tv_usec++; \
